using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SirZubair2
{
    class Program
    {
        static void Main(string[] args)
        {
          
            int a = Convert.ToInt16(Console.ReadLine());
            int b = Convert.ToInt16(Console.ReadLine());
            
            
            for (int i = 1; i <= 15; i++)
            {
                
                int c = a + b;
                Console.WriteLine(c);
                a = b;
                b =c;
            
            }
        }
    }
}
