using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication3
{
    class Program
    {
        static void Main(string[] args)
        {
            int i = 1;
          
        start:
            Console.WriteLine("Hi");
        i++;
        if (i < 10)
        {
            goto start;
        }
               
        }
    }
}
