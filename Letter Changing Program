using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SirZubair3
{
    class Program
    {
        static void Main(string[] args)
        {

            string x = Console.ReadLine();
            for (int i = (x.Length - 1); i >= 0; i--)
            {
                Console.Write(x[i]);

            }

            Console.WriteLine();
        }
                   
    }
}
